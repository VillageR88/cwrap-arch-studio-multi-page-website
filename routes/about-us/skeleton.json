{
  "head": {
    "link": [
      {
        "rel": "shortcut icon",
        "href": "../favicon.ico",
        "type": "image/x-icon"
      }
    ]
  },
  "element": "cwrap-template",
  "text": "cwrapTemplate[body(prefix=../,script=cwrapOmit,routeTitle=ABOUT US)]",
  "passover": [
    {
      "element": "main",
      "style": "display: flex; flex-direction: column; margin-inline: auto; margin-top: 56px; max-width: 1110px; width: 100%;",
      "children": [
        {
          "element": "cwrap-template",
          "text": "cwrapTemplate[commonHero]"
        },
        {
          "element": "div",
          "style": "display: flex; justify-content: space-between; margin-top: 200px;",
          "children": [
            {
              "element": "div",
              "style": "display: flex; flex-direction: column; max-width: 446px; white-space: pre-wrap;",
              "children": [
                {
                  "element": "div",
                  "style": "border-bottom: #C8CCD8 solid 1px; width: 65px;"
                },
                {
                  "element": "h2",
                  "text": "cwrapGlobal[about-us.heritage.title]",
                  "style": "color: #1B1D23; margin-top: 83px;"
                },
                {
                  "element": "p",
                  "text": "cwrapGlobal[about-us.heritage.description]",
                  "style": "color: #60636D; margin-top: 51px;"
                }
              ]
            },
            {
              "element": "img",
              "attributes": {
                "src": "../cwrapGlobal[about-us.heritage.image]",
                "alt": "cwrapGlobal[about-us.heritage.title] image"
              }
            }
          ]
        },
        {
          "element": "div",
          "style": "display: flex; justify-content: space-between; margin-block: 200px 159px;",
          "children": [
            {
              "element": "h2",
              "style": "color: #1B1D23; white-space: pre-wrap;",
              "text": "cwrapGlobal[about-us.leaders.title]"
            },
            {
              "element": "div",
              "style": "display: flex; flex-wrap: wrap; gap: 63px 30px; max-width: 730px;",
              "blueprint": {
                "count": "4",
                "element": "div",
                "style": "display: flex; flex-direction: column; gap: 15px;",
                "children": [
                  {
                    "element": "div",
                    "style": "display: flex; position: relative;",
                    "children": [
                      {
                        "element": "div",
                        "style": "align-items: center; display: flex; height: 100%; justify-content: center; opacity: 0; position: absolute; transition: 300ms; width: 100%;",
                        "extend": [
                          {
                            "extension": ":hover",
                            "style": "background-color: rgba(0,0,0,0.5); opacity: 1;"
                          }
                        ],
                        "children": [
                          {
                            "element": "cwrap-template",
                            "text": "cwrapTemplate[ulSocial(iconFill=white)]"
                          }
                        ]
                      },
                      {
                        "element": "img",
                        "attributes": {
                          "src": "../cwrapGlobal[about-us.leaders.gallery.cwrapIndex.src]",
                          "alt": "cwrapGlobal[about-us.leaders.gallery.cwrapIndex.title] image"
                        }
                      }
                    ]
                  },
                  {
                    "element": "section",
                    "style": "display: flex; flex-direction: column;",
                    "children": [
                      {
                        "element": "h3",
                        "style": "color: #1B1D23;",
                        "text": "cwrapGlobal[about-us.leaders.gallery.cwrapIndex.title]"
                      },
                      {
                        "element": "p",
                        "style": "color: rgba(27, 29, 35, 0.75);",
                        "text": "cwrapGlobal[about-us.leaders.gallery.cwrapIndex.description]"
                      }
                    ]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  ]
}