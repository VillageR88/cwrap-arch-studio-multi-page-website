  "classroom": [
    {
      "name": "@view-transition",
      "type": "custom",
      "style": "navigation: auto;"
    },
    {
      "name": "html",
      "type": "element",
      "style": "overflow-x: clip; scroll-behavior: smooth;",
      "mediaQueries": [
        {
          "query": "max-width: 768px",
          "style": "overflow-x: unset;"
        }
      ]
    },
    {
      "name": "body",
      "type": "element",
      "style": "overflow-x: clip; scroll-behavior: smooth;",
      "mediaQueries": [
        {
          "query": "max-width: 768px",
          "style": "overflow-x: unset;"
        }
      ]
    },
    {
      "name": "h1",
      "type": "element",
      "style": "font-size: 250px; font-weight: bold; letter-spacing: -5px; line-height: 200px; margin: 0;",
      "mediaQueries": [
        {
          "query": "max-width: 1280px",
          "style": "font-size: 120px; letter-spacing: -3px; line-height: 200px;"
        },
        {
          "query": "max-width: 768px",
          "style": "display: none;"
        }
      ]
    },
    {
      "name": "h2",
      "type": "element",
      "style": "font-size: 72px; font-weight: bold; letter-spacing: -2px; line-height: 64px; margin: 0;",
      "mediaQueries": [
        {
          "query": "max-width: 768px",
          "style": "font-size: 48px; letter-spacing: -1.71px; line-height: 52px;"
        }
      ]
    },
    {
      "name": "h3",
      "type": "element",
      "style": "font-size: 32px; font-weight: bold; line-height: 40px; margin: 0;"
    },
    {
      "name": "p",
      "type": "element",
      "style": "font-size: 18px; font-weight: 500; line-height: 24px; margin: 0;"
    }
  ],
