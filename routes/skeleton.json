          "children": [
            {
              "element": "h1",
              "text": "cwrapGlobal[home.welcome.h1]",
              "style": "color: #EEEFF4; font-size: 250px; font-weight: bold; letter-spacing: -5px; line-height: 200px; margin: 0; position: absolute; top: 0;"
